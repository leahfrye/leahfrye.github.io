{
  "name": "new-portfolio",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://leahfrye.github.io",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.2.2",
    "popper.js": "^1.14.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-images": "^0.5.17",
    "react-scripts": "1.1.4",
    "sass": "^1.3.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
